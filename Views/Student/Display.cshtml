@model IEnumerable<ContosoUniversity.Models.Student>

<h2>Student Display</h2>

<table class="table table-hover">
    <tr class=" text-light bg-dark">
        <th>@Html.DisplayNameFor(model => model.LastName)</th>
        <th>@Html.DisplayNameFor(model => model.FirstMidName)</th>
        <th>@Html.DisplayNameFor(model => model.EnrollmentDate)</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(model => item.LastName)</td>
            <td>@Html.DisplayFor(model => item.FirstMidName)</td>
            <td>@Html.DisplayFor(model => item.EnrollmentDate)</td>
        </tr>
    }
</table>